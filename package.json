{
  "name": "lifesprint",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/browser": "^7.120.0",
    "@sentry/react": "^7.120.0",
    "@sentry/tracing": "^7.120.0",
    "@supabase/supabase-js": "^2.49.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/workbox-webpack-plugin": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "typescript": "^5.0.0",
    "web-vitals": "^2.1.4",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "generate-pwa-assets": "node scripts/generate-icons.js && node scripts/generate-screenshots.js",
    "check-pwa": "node scripts/check-pwa.js",
    "pwa-setup": "npm run generate-pwa-assets && npm run check-pwa",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prebuild": "npm run lint && npm run type-check",
    "postbuild": "npm run analyze"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-webpack5": "^7.0.0",
    "@tailwindcss/postcss": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.13",
    "chalk": "^5.4.1",
    "cypress": "^12.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^11.0.1",
    "prettier": "^2.8.0",
    "source-map-explorer": "^2.5.0",
    "tailwindcss": "^3.2.4",
    "terser-webpack-plugin": "^5.3.9",
    "workbox-webpack-plugin": "^7.3.0"
  }
}
